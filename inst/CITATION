citHeader("To cite alien in publications use:")

citEntry(entry = "Article",
  title        = "alien",
  author       = personList(as.person("Dominique Gravel"), as.person("Ignasi Bartomeus"), as.person("...")),
  journal      = "...",
  year         = "2017",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "",

  textVersion  =
  paste("alien",
        "2017",
        "Gravel, ...",
        "...")
)
